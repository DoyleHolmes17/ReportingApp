<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textareaInfra"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="2dip"
        android:paddingTop="6dip"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textalamatInfra"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="2dip"
        android:textColor="#000000"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textcategorysInfra"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="2dip"
        android:textColor="#000000"
        android:textSize="12sp" />
    <TextView
        android:id="@+id/textpelaporInfra"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="2dip"
        android:textSize="14sp"
        android:textColor="#A52A2A"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textteknisiInfra"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="2dip"
        android:textSize="12sp"
        android:textColor="#000000" />
    <TextView
        android:id="@+id/textstartInfra"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="2dip"
        android:textSize="12sp"
        android:textColor="#000000" />
    <TextView
        android:id="@+id/textendInfra"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="2dip"
        android:textSize="12sp"
        android:textColor="#000000" />
    <TextView
        android:id="@+id/textlamaInfra"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="2dip"
        android:textSize="12sp"
        android:textColor="#000000" />



    <TextView
        android:id="@+id/textnoteInfra"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="2dip"
        android:textSize="14sp"
        android:textColor="#000000" />
    <TextView
        android:id="@+id/textnoteadminInfra"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="2dip"
        android:textSize="14sp"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/textnoteteknisInfra"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="2dip"
        android:textSize="14sp"
        android:textColor="#000000" />


    <TextView
        android:id="@+id/textstatusReport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#5d5d5d"
        android:textStyle="bold" />
</LinearLayout>